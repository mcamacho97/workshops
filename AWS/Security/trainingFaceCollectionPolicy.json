{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "S3Statement",
        "Effect": "Allow",
        "Action": [
            "s3-object-lambda:GetObject",
            "s3:GetObject",
            "s3:ListBucket"
        ],
        "Resource": [
            "arn:aws:s3:::trainingfacecollection-809489680864",
            "arn:aws:s3:::trainingfacecollection-809489680864/*"
        ]
      },
      {
        "Sid": "RekognitionStatement",
        "Effect": "Allow",
        "Action": "rekognition:IndexFaces",
        "Resource": "*"
      }
    ]
  }