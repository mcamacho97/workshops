{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "S3Statement",
        "Effect": "Allow",
        "Action": [
            "s3-object-lambda:GetObject",
            "s3-object-lambda:PutObject",
            "s3:GetObject",
            "s3:PutObject"
        ],
        "Resource": [
            "arn:aws:s3:::*",
            "arn:aws:s3:::/*"
        ]
      }
    ]
  }